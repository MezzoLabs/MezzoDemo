{
    "name": "mezzolabs/mezzo",
    "description": "Mezzo - mix your content with a sexy backend.",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "mezzo",
        "cms",
        "cmf",
        "admin",
        "acp",
        "generator"
    ],
    "homepage": "https://github.com/MezzoLabs/Mezzo",
    "authors": [
        {
            "name": "Simon Schneider, Marc Vornetran",
            "email": "mail@mezzolabs.io",
            "homepage": "http://mezzolabs.io",
            "role": "Developers"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "dingo/api": "0.9.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "mezzolabs\\mezzo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mezzoLabs\\mezzo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
