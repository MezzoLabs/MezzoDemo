{
    "name": "MezzoLabs/Mezzo",
    "description": "Mezzo - mix your content with a sexy backend.",
    "keywords": [
        "laravel",
        "mezzo",
        "cms",
        "cmf",
        "admin",
        "acp",
        "generator"
    ],
    "homepage": "https://github.com/MezzoLabs/Mezzo",
    "authors": [
        {
            "name": "Simon Schneider",
            "email": "simon@mezzolabs.io",
            "homepage": "mezzolabs.io",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "MezzoLabs\\Mezzo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MezzoLabs\\Mezzo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
